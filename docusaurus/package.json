{
  "name": "physical-ai-textbook",
  "version": "1.0.0",
  "description": "Interactive textbook on Physical AI and Humanoid Robotics",
  "author": "PIAIC",
  "license": "MIT",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "chatbot:index": "npx tsx scripts/index-content.ts",
    "db:init": "npx tsx scripts/init-db.ts",
    "api": "npx tsx server/api.ts",
    "dev": "concurrently \"npm run start\" \"npm run api\""
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/plugin-pwa": "^3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@neondatabase/serverless": "^1.0.2",
    "@qdrant/js-client-rest": "^1.16.2",
    "@xenova/transformers": "^2.17.2",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "openai": "^6.16.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@vercel/node": "^5.5.23",
    "concurrently": "^9.2.1",
    "ignore-loader": "^0.1.2",
    "tsx": "^4.21.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
